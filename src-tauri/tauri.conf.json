{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauriseqta",
  "version": "0.1.0",
  "identifier": "com.tauriseqta.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "tauriseqta",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/betterseqta-dark-icon.png",
      "icons/betterseqta-light-icon.png",
      "icons/betterseqta-light-outline.png",
      "icons/icon-32.png",
      "icons/icon-48.png",
      "icons/icon-64.png",
      "icons/icon.ico"
    ],
    "windows": {
      "nsis": {
        "installMode": "currentUser"
      }
    }
  }
}